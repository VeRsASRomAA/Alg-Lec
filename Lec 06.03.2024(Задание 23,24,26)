#24

x = int(input("Введите число: "))
y = int(input("Введите число: "))
if x > 0 and y > 0:
    print("1")
if x < 0 and y > 0:
    print("2 ")
if x < 0 and y < 0:
    print("3")
if x > 0 and y < 0:
    print("4 ")

#26

x = int(input("Введите число: "))
y = int(input("Введите число: "))
if x > 0 and y > 0 or x < 0 and y > 0:
    print("координата находится в полуплоскости")
else:
    print("нет")